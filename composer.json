{
    "name": "flowti/zabbix-bundle",
    "description": "Zabbix integration for Symfony",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": ">=4.0",
        "guzzlehttp/guzzle": ">=6.3",
        "symfony/monolog-bundle": ">=3.1",
        "symfony/dotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Flowti\\ZabbixBundle\\": "src/Flowti/"
            
